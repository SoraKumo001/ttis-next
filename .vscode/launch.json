{
  "version": "0.2.0",
  "configurations": [
      {
          "name": "Debug Jest Tests",
          "type": "node",
          "request": "launch",
          "cwd": "${workspaceRoot}/ttis-back",
          "program": "${workspaceRoot}/ttis-back/node_modules/.bin/jest",
          "protocol": "inspector",
          "args": [ "--runInBand" ],
          "windows": {
              "program": "${workspaceRoot}/ttis-back/node_modules/jest/bin/jest.js"
          },
          "console": "integratedTerminal",
          "internalConsoleOptions": "neverOpen"
      }
  ]
}