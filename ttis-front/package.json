{
  "name": "ttis-front",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node -P server/tsconfig.json server/index.ts",
    "build": "tsc -b server && next build",
    "start": "cross-env NODE_ENV=production node .next/index.js",
    "start-docker": "docker-compose -f docker/docker-compose.yml up -d --build",
    "export": "next build && next export",
    "storybook": "start-storybook",
    "reg-local": "node .github/scripts/reg-local",
    "capture": "npm-run-all -p -r sc:*",
    "sc:start-storybook": "start-storybook -p 9001 --ci",
    "sc:screenshot": "storycap http://localhost:9001 --stateChangeDelay 3000 --serverCmd \"yarn start-storybook -p 9001 --ci\"",
    "graphql-gen": "graphql-codegen --config graphql/codegen.json"
  },
  "dependencies": {
    "apollo-boost": "^0.4.7",
    "apollo-link-context": "^1.0.20",
    "axios": "^0.19.2",
    "connect-redis": "^4.0.4",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.3",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^9.3.5",
    "react": "^16.13.1",
    "react-apollo": "^3.1.5",
    "react-dom": "^16.13.1",
    "redis": "^3.0.2",
    "ts-node": "^8.9.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.13.3",
    "@graphql-codegen/introspection": "^1.13.3",
    "@graphql-codegen/typescript": "^1.13.3",
    "@graphql-codegen/typescript-operations": "^1.13.3",
    "@graphql-codegen/typescript-react-apollo": "^1.13.3",
    "@jswf/react": "^1.0.12",
    "@types/connect-redis": "^0.0.13",
    "@types/express": "^4.17.6",
    "@types/express-session": "^1.17.0",
    "@types/node": "^13.13.2",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/redis": "^2.8.18",
    "@types/request": "^2.48.4",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.8.3",
    "url-loader": "^4.1.0"
  }
}
