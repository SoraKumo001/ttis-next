{
  "name": "ttis-front",
  "version": "1.0.0",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "tsc -b server && next build",
    "capture": "npm-run-all -p -r sc:*",
    "dev": "ts-node -P server/tsconfig.json server/index.ts",
    "export": "next build && next export",
    "generate": "graphql-codegen --config graphql/codegen.json",
    "reg-local": "node .github/scripts/reg-local",
    "sc:screenshot": "storycap http://localhost:9001 --stateChangeDelay 3000 --serverCmd \"yarn start-storybook -p 9001 --ci\"",
    "sc:start-storybook": "start-storybook -p 9001 --ci",
    "start": "cross-env NODE_ENV=production node .next/index.js",
    "start-docker": "docker-compose -f docker/docker-compose.yml up -d --build",
    "storybook": "start-storybook"
  },
  "dependencies": {
    "@apollo/client": "^3.3.4",
    "@apollo/react-ssr": "^4.0.0",
    "@jswf/react": "^1.0.33",
    "apollo-upload-client": "^14.1.3",
    "axios": "^0.21.0",
    "connect-redis": "^5.0.0",
    "cross-env": "^7.0.3",
    "dateformat": "^4.3.1",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "extract-files": "^9.0.0",
    "graphql": "^15.4.0",
    "next": "^10.0.3",
    "redis": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2",
    "webpack": "^5.10.0"
  },
  "devDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-resize-observer": "^1.1.1",
    "@graphql-codegen/cli": "^1.19.4",
    "@graphql-codegen/introspection": "^1.18.1",
    "@graphql-codegen/typescript": "^1.19.0",
    "@graphql-codegen/typescript-operations": "^1.17.12",
    "@graphql-codegen/typescript-react-apollo": "^2.2.1",
    "@types/apollo-upload-client": "^14.1.0",
    "@types/connect-redis": "^0.0.15",
    "@types/dateformat": "^3.0.1",
    "@types/express": "^4.17.9",
    "@types/express-session": "^1.17.3",
    "@types/node": "^14.14.11",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/redis": "^2.8.28",
    "@types/request": "^2.48.5"
  },
  "resolutions": {
    "webpack": "^5.10.0"
  }
}
