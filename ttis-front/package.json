{
  "name": "ttis-front",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node -P server/tsconfig.json server/index.ts",
    "build": "tsc -b server && next build",
    "start": "cross-env NODE_ENV=production node .next/index.js",
    "start-docker": "docker-compose -f docker/docker-compose.yml up -d --build",
    "export": "next build && next export",
    "storybook": "start-storybook",
    "reg-local": "node .github/scripts/reg-local",
    "capture": "npm-run-all -p -r sc:*",
    "sc:start-storybook": "start-storybook -p 9001 --ci",
    "sc:screenshot": "storycap http://localhost:9001 --stateChangeDelay 3000 --serverCmd \"yarn start-storybook -p 9001 --ci\"",
    "generate": "graphql-codegen --config graphql/codegen.json"
  },
  "dependencies": {
    "@types/dateformat": "^3.0.1",
    "apollo-boost": "^0.4.9",
    "apollo-link-context": "^1.0.20",
    "axios": "^0.20.0",
    "connect-redis": "^5.0.0",
    "cross-env": "^7.0.2",
    "dateformat": "^3.0.3",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "graphql": "^15.3.0",
    "next": "^9.5.4",
    "react": "^16.13.1",
    "react-apollo": "^3.1.5",
    "react-dom": "^16.13.1",
    "redis": "^3.0.2",
    "ts-node": "^9.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.17.8",
    "@graphql-codegen/introspection": "^1.17.8",
    "@graphql-codegen/typescript": "^1.17.9",
    "@graphql-codegen/typescript-operations": "^1.17.8",
    "@graphql-codegen/typescript-react-apollo": "^2.0.6",
    "@jswf/react": "^1.0.23",
    "@types/apollo-upload-client": "^14.1.0",
    "@types/connect-redis": "^0.0.14",
    "@types/express": "^4.17.8",
    "@types/express-session": "^1.17.0",
    "@types/node": "^14.10.1",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/redis": "^2.8.27",
    "@types/request": "^2.48.5",
    "@types/webpack": "^4.41.22",
    "apollo-upload-client": "^14.1.2",
    "react-resize-observer": "^1.1.1",
    "typescript": "^4.0.2",
    "url-loader": "^4.1.0"
  }
}
